{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "env": {
    "OPENAI_API_KEY": "${OPENAI_API_KEY}",
    "NEXT_PUBLIC_VERCEL_URL": "${VERCEL_URL}"
  }
}
